/* 已提醒 */

.remind {
    background-color: #161824 !important;
    color: #fff !important;
}

/* 确认收货 */

.color {
    color: #d33d3c !important;
    border: 1rpx solid #d33d3c;
}

.btnOrder .btnCust image {
    width: 42rpx;
    height: 34rpx;
    float: right;
    margin-right: 10rpx;
    margin-top: 20rpx;
    z-index: 99;
}

/* 底部 */

.btn_list {
    width: 750rpx;
    /* height: 130rpx; */
    line-height: 130rpx;
    position: fixed;
    bottom: 0;
    background: rgba(22, 24, 36, 1);
    border-top: 1rpx solid rgba(255, 255, 255, 0.08);
}

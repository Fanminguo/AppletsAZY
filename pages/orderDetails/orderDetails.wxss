/* pages/orderDetails/orderDetails.wxss */

page {
    background: #161824;
}

.yiciClass {
    width: 710rpx;
    height: 68rpx;
    background: rgba(29, 31, 43, 1);
    border-radius: 8rpx;
    margin: 0 auto;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.yiciClass .logoYici {
    width: 51rpx;
    height: 46rpx;
    margin-top: 8rpx;
    margin-left: 32rpx;
}

.yiciClass .jiantou {
    width: 14rpx;
    height: 24rpx;
    margin-top: 24rpx;
    margin-right: 35rpx;
}

.yiciClass .textNum {
    margin-left: 10rpx;
    font-size: 28rpx;
    color: rgba(255, 255, 255, 0.6);
}

.yiciClass .textNum text {
    height: 68rpx;
    line-height: 68rpx;
    font-family: PingFangHK-Regular, PingFangHK;
    font-weight: 400;
    color: #d33d34;
}

/* 顶部背景色 */

.list_top_box {
    background-color: #161824;
    height: 150rpx;
    position: relative;
}

/* 收货人信息 */

.boxName {
    width: 700rpx;
    border-radius: 12rpx;
    margin: 0 auto;
    position: relative;
    top: 30rpx;
    z-index: 1;
    display: block;
    background: #1d1f28;
    height: 155rpx;
}

.boxName .information {
    margin: 0 0 0 30rpx;
    padding: 30rpx 30rpx 15rpx 70rpx;
    color: rgba(255, 255, 255, 0.9);
}

/* 收货 */

.boxName .information .receiving {
    font-size: 32rpx;
}

/* 姓名 */

.boxName .information .name {
    font-size: 28rpx;
    margin-left: 10rpx;
}

/* 手机号 */

.boxName .information .number {
    font-size: 28rpx;
    margin-left: 20rpx;
}

.boxName image {
    position: absolute;
    left: 23rpx;
    top: 51rpx;
    width: 53rpx;
    height: 53rpx;
}

/* 地址 */

.address {
    font-size: 28rpx;
    line-height: 40rpx;
    color: rgba(255, 255, 255, 0.9);
    margin-left: 30rpx;
    margin-bottom: 24rpx;
    padding-left: 70rpx;
    overflow: hidden; /*超出部分隐藏*/
    white-space: nowrap; /*不换行*/
    text-overflow: ellipsis; /*超出部分文字以...显示*/
}

/* 列表 */

.total {
    margin-top: 59rpx;
}

.cont_count {
    padding-top: 6rpx;
    position: relative;
    width: 700rpx;
    text-align: center;
    margin: 0 auto;
    margin-top: 25rpx;
    background-color: #1d1f28;
    border-radius: 24rpx;
}

/* 编号 */

.seria {
    position: relative;
    display: inline-block;
    width: 90%;
    height: 70rpx;
    line-height: 70rpx;
    /* background-color: yellow; */
}

/* 订单lab */

.seriac_name {
    position: absolute;
    font-size: 28rpx;
    color: rgba(255, 255, 255, 0.9);
    left: 0rpx;
}

.seriac_name image {
    width: 27rpx;
    height: 27rpx;
}

.seriac_name text {
    margin-left: 15rpx;
}

/* 订单状态 */

.seriac_start {
    position: absolute;
    right: 0rpx;
    color: #d33d3c;
    font-size: 28rpx;
}

.seriac_start image {
    width: 37rpx;
    height: 37rpx;
}

/* 列表 */

.list {
    position: relative;
    width: 90%;
    display: inline-block;
    padding-bottom: 10rpx;
    height: 200rpx;
}

/* 按钮颜色 */

.color {
    color: #d33d3c !important;
    border: 1rpx solid #d33d3c;
}

/* 列表图片 */

.list_img {
    position: absolute;
    width: 144rpx;
    height: 144rpx;
    left: 0rpx;
    top: 30rpx;
}

/* 列表标题 */

.list_name {
    position: absolute;
    text-align: left;
    top: 30rpx;
    left: 180rpx;
    color: rgba(255, 255, 255, 0.9);
    width: 350rpx;
    font-size: 28rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

/* 规格 */

.list_ions {
    position: absolute;
    text-align: left;
    top: 70rpx;
    left: 180rpx;
    color: rgba(255, 255, 255, 0.6);
    width: 350rpx;
    font-size: 24rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

/* 列表价格 */

.list_money {
    position: absolute;
    font-size: 28rpx;
    left: 170rpx;
    bottom: 30rpx;
    color: #d33d3c;
}

/* 编号 */

.order {
    font-size: 24rpx;
    color: rgba(255, 255, 255, 0.9);
    margin-left: 30rpx;
    text-align: left;
}

/* 数量 */

.goodsNum {
    float: right;
    color: rgba(255, 255, 255, 0.6);
    font-size: 24rpx;
    margin-top: 30rpx;
}

/* 标签功能 */

.coent_list {
    width: 700;
    height: 130rpx;
    line-height: 130rpx;
    background-color: #1d1f2b;
    border-radius: 24rpx;
    margin: 0 auto;
}

/* 按钮 */

.coent_del {
    float: right;
    margin-right: 20rpx;
    display: inline-block;
    width: 170rpx;
    text-align: center;
    height: 68rpx;
    border: 1rpx solid rgba(255, 255, 255, 0.4);
    font-size: 28rpx;
    line-height: 68rpx;
    margin-top: 17rpx;
    border-radius: 10rpx;
    color: rgba(255, 255, 255, 0.6);
    margin-bottom: 19rpx;
}

.coent_two {
    margin-top: 0 !important;
}

.coent_del .coentText {
    float: right;
    display: inline-block;
    width: 170rpx;
    text-align: center;
    height: 68rpx;
    font-size: 28rpx;
    line-height: 68rpx;
    border-radius: 10rpx;
    color: #555;
}

/* 价格信息 */

.priceInform {
    width: 700rpx;
    border-radius: 12rpx;
    background: #1d1f2b;
    margin: 0 auto;
    margin-top: 25rpx;
    padding-bottom: 20rpx;
}

.priceInform .price1 {
    padding-top: 10rpx;
}

.priceInform .price {
    margin: 0 auto;
    font-size: 28rpx;
    overflow: hidden;
    padding-left: 30rpx;
    padding-right: 30rpx;
    line-height: 50rpx;
}

.priceInform .price .name {
    color: rgba(255, 255, 255, 0.6);
}

.priceInform .price .money {
    float: right;
    color: rgba(255, 255, 255, 0.6);
}

.price .realName {
    color: rgba(255, 255, 255, 0.75);
    font-size: 32rpx;
}

.price .realMoney {
    color: #f9493c;
    float: right;
    font-size: 32rpx;
}

.h20 {
    line-height: 10rpx;
}

.h20 image {
    width: 100%;
    height: 12rpx;
}

.fff75 {
    color: rgba(255, 255, 255, 0.75);
}

/* 订单信息 */

.orderInform {
    background: #1d1f2b;
    width: 700rpx;
    margin: 0 auto;
    margin-top: 25rpx;
    border-radius: 12rpx;
    margin-bottom: 170rpx;
}

/* .topInform {
    font-size: 30rpx;
    color: #666;
    padding: 15rpx;
}

.topInform text {
    color: #d33d3c;
    margin-left: 20rpx;
} */

.itemOrder {
    font-size: 28rpx;
    color: rgba(255, 255, 255, 0.6);
    padding: 10rpx 0 10rpx 20rpx;
}

/* 猜你喜欢 */

.likeList {
    width: 700rpx;
    border-radius: 12rpx;
    background: #fff;
    margin: 0 auto;
    margin-top: 25rpx;
    margin-bottom: 150rpx;
}

.likeList .topHerder {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 32rpx;
}

.orderList {
    width: 330rpx;
    float: left;
    border-radius: 8rpx;
    background: #fff;
    border: 1rpx solid rgba(0, 0, 0, 0.1);
    box-sizing: border-box;
    padding-bottom: 12rpx;
    margin: 0 0 20rpx 14rpx;
}

.orderList image {
    height: 330rpx;
    width: 100%;
    outline: 0 none;
    border-radius: 8rpx 8rpx 0 0;
}

.orderList .orderName {
    font-size: 30rpx;
    line-height: 40rpx;
    padding-top: 18rpx;
    padding-right: 32rpx;
    color: #333;
    padding-left: 20rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

/* 底部金额 */

.orderList .orderDrief {
    color: #8b8b8b;
    font-size: 24rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    text-indent: 20rpx;
    width: 330rpx;
    height: 60rpx;
}

.btnOrder .btnMoney {
    font-size: 30rpx;
    color: #f9493c;
    padding-left: 20rpx;
    float: left;
    line-height: 60rpx;
}


/* pages/cart/cart.wxss */
.store-top{ height: 88rpx; line-height: 88rpx; border-bottom: solid 1rpx rgba(255,255,255,0.1); position: relative; padding-left: 100rpx;
font-size: 24rpx;color:rgba(255,255,255,0.9);}
.ckstore{ position: absolute; left: 30rpx; width: 42rpx; height: 42rpx; top: -2rpx;}
.products-info{ position: relative;border-bottom: 1px solid rgba(0,0,0,0.06); padding: 24rpx 50rpx 24rpx 290rpx; min-height: 200rpx; font-size: 28rpx; color: #111; width: 420rpx;}
.products-img{ position: absolute; left: 96rpx; width: 168rpx; height: 168rpx; border-radius: 12rpx; top: 24rpx}
.ckproduct{position: absolute; left: 30rpx; width: 42rpx; height: 42rpx; top: 90rpx}
.products-name{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;color:rgba(255,255,255,0.9);}
.products-js{    overflow: hidden;white-space: nowrap;width: 340rpx;height: 60rpx;font-size: 24rpx;line-height: 60rpx;
   color:rgba(255,255,255,0.6);text-overflow: ellipsis; margin-top: 20rpx;
border-radius:27rpx;}
.products-total{margin-top: 13rpx}
.products-price{color: #d33d3c; font-size: 34rpx; }
.products-price text{ font-size: 20rpx; margin-left:10rpx}
.products-mum{ font-size: 24rpx; margin-top: 5rpx;color:rgba(255,255,255,0.9);}
.products-mum text{font-size: 28rpx; margin-left: 10rpx}
.cart-bottom{ height: 100rpx; line-height: 100rpx; position: fixed; width: 100%; left: 0; bottom: 0; background: #000; font-size: 32rpx; z-index: 50}
.cart-btn{ width: 25%;background-color: #d33d3c; color: #fff; text-align: center}
.cart-bottom-left{ width: 75%;color:rgba(255,255,255,0.9);}
.cart-price{ padding-right: 30rpx;}
.ckall{ margin-left: 30rpx; display: inline-block}
.total-price{ color: #d33d3c}
.cart-price-icon{ color:#d33d3c; font-size: 24rpx;}
.store-chbox{ display: inline-block;}
.edit-btn{ text-align: right; margin-right: 20rpx; color: #d33d3c}
.products-list{ position: relative}
.del-cart{ height: 250rpx; line-height: 250rpx; text-align: center;width: 130rpx;background-color: #d33d3c; color: #fff; float: right;
position: absolute;  right: -130rpx; top: 0; z-index: 10;}
.value-box{ float:right; margin-top: -29rpx;}



.value-box .add,.value-box .minus{width:40rpx;height:40rpx; display: inline-block; border-radius: 50%;background:rgba(146,147,152,1);
background:rgba(57,59,68,1); text-align: center; line-height: 40rpx;color:rgba(194,195,198,1); font-size: 28rpx;}
.value-box .buy-num {display: inline-block;color:rgba(194,195,198,1);width: 80rpx;text-align: center;font-size: 30rpx;}
.value-box .minus{font-size: 32rpx; line-height: 34rpx}

.edit .value-box .add,.edit .value-box .minus{display: inline-block;vertical-align: top; width:60rpx; height: 62rpx;
text-align: center; line-height: 60rpx; color: rgba(255,255,255,0.9); background: none}
.edit .value-box .buy-num {width: 164rpx;height:66rpx;line-height: 70rpx;font-size: 30rpx;border-width: 0 1px;
    color: #666;font-weight: 500;text-align: center;border-color: #e2e2e2; border-right: solid 1px rgba(255,255,255,0.4);
    border-left: solid 1px rgba(255,255,255,0.4);}
.edit .products-name{ display: none}
/* .edit .products-js{ display: none} */
.edit .products-total {display: none}
.edit .del-cart{ display: block}
.edit .products-js{position: absolute;top: 130rpx;left:317rpx;color: #999;font-size: 24rpx;overflow: hidden;
    text-overflow: ellipsis;white-space: nowrap;border: solid 1px #d9d9d9;height: 60rpx;line-height: 60rpx;
    width: 290rpx;padding: 0 6rpx;}
.edit .value-box{ display: block;top: 30rpx;position: absolute;z-index: 1;left:317rpx;border: 1px solid rgba(255,255,255,0.4);
  border-radius: 0;height: 64rpx;float: right; width: 290rpx;}
/* .cont{ margin-bottom: 100rpx} */
.cart-null{ text-align: center;}
.cart-null text{  color:rgba(255,255,255,0.6); font-size: 28rpx;}
.cart-null-text{color:rgba(255,255,255,0.9);text-align: center;margin-top: 40rpx;font-size: 34rpx; margin-bottom: 20rpx;}
.cart-null navigator{ color:#EBA925; font-size: 28rpx; display: inline } 
.cart-null{ margin-top: 600rpx}

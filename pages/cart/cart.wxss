.cart-store{ background: #1D1F2B; margin-bottom: 20rpx;}
.cart-bottom checkbox .wx-checkbox-input {background: url(https://bj.aizhiyi.com/wap/test/wap/xcxImg/70.png) no-repeat center; border:0; background-size: cover;}
.cart-bottom checkbox .wx-checkbox-input.wx-checkbox-input-checked {background: url(https://bj.aizhiyi.com/wap/test/wap/xcxImg/69.png) no-repeat center; background-size: cover;}
.store-name{background: url(https://bj.aizhiyi.com/wap/test/wap/xcxImg/100.png) no-repeat left center; display: block; float: left; background-size: 26rpx 25rpx; padding-left: 40rpx;}

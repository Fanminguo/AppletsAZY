/* pages/live/report/report.wxss */
.report-title{color:rgba(255,255,255,0.6); font-size: 24rpx; padding:0 30rpx; line-height: 60rpx;}
.report-list{ height: 86rpx; color:rgba(255,255,255,0.9);font-size:28rpx; line-height: 86rpx; padding:0 30rpx; position: relative;border-bottom: solid 2rpx rgba(255, 255, 255, 0.04);}
.report-list-icon{background:url("https://bj.aizhiyi.com/wap/test/wap/xcxImg/67.png") no-repeat center center; width: 34rpx; height: 34rpx; position: absolute; right: 30rpx; top: 26rpx; background-size: cover; }

.imgDialog{ position: fixed; width: 100%; height: 100%; background-color: rgba(0,0,0,0.5); left: 0; top: 0;}
.imgDialog-cont{width:540rpx;height:365rpx;background:rgba(37,39,53,1);box-shadow:0px 1rpx 3px 0px rgba(0,0,0,0.15);border-radius:26rpx;
position: absolute; left: 50%; margin-left: -270rpx; top: 20%; }
.imgDialog-cont-text{ font-size: 26rpx; text-align: center; color:rgba(255,255,255,0.9);}
.imgDialog-cont-text text{ color: #CA493A;}
.imgDialog-contTe{ line-height: 50rpx; padding: 30rpx 0 0 0;}
.imgDialog-img{width:82rpx;height:86rpx; display: block; margin:20rpx auto}
.imgDialog-bottom{ height: 86rpx; font-size: 28rpx; line-height:86rpx; border-top: solid 1rpx #999; position: absolute; left: 0; bottom: 0;
width: 100%;}
.imgDialog-bottom-return{ float: left; width: 50%;color:rgba(255,255,255,0.7); text-align: center;border-right:solid 1rpx #999}
.imgDialog-bottom-ok{ float: left; width: 49%;color:rgba(202,73,58,1); text-align: center;}

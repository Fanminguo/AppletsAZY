/*弹出框  */

.shelter {
    position: fixed;
    z-index: 9999;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    filter: progid:DXImageTransform.Microsoft.gradient(enabled='true', startColorstr='#BF000000', endColorstr='#BF000000');
    background: rgba(0, 0, 0, 0.75);
    background: rgba(0, 0, 0, 0.6);
    height: 100% !important;
}

.wrapper {
    width: 560rpx;
    height: 747rpx;
    background-image: url(https://bj.aizhiyi.com/wap/images/me_icon/long_dialog.png?111);
    top: 50rpx !important;
    background-size: 100% 100%;
    background-position: bottom;
    margin: 0 auto;
}

.s-dialog-content {
    min-height: 100rpx;
    margin-top: 120rpx;
    padding: 0 5rpx;
    text-align: center;
    font-size: 28rpx;
    line-height: 42rpx;
}

.s-dialog-content .tips {
    width: 500rpx;
    text-align: center;
    font-size: 28rpx;
    color: #333;
    margin: 0 auto;
    line-height: 100rpx;
    letter-spacing: 1rpx;
    margin-left: 10%;
}

.s-dialog-content label {
    display: block;
    padding-top: 45rpx;
}

.s-dialog-btn-wapper view {
    display: inline-block;
    width: 49%;
    font-size: 34rpx;
    font-weight: normal;
    line-height: 48rpx;
    text-align: center;
    color: #666;
}

.s-dialog-btn-cancel {
    color: #999 !important;
}

.s-dialog-btn-wapper view span {
    display: block;
    width: 190rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    margin: 0 auto;
    border-radius: 8rpx;
}

.s-dialog-btn-wapper .s-dialog-btn-ok span {
    background-color: #d33d3c;
    color: #fff;
}

.s-dialog-btn-wapper .s-dialog-btn-cancel span {
    border: 2rpx solid #d33d3c;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    color: #d33d3c;
}

.s-dialog-content radio {
    float: right;
    margin-right: 20rpx;
    /* width: 34rpx;
	height: 34rpx; */
}

/* 全部订单去支付样式 */

.btnClass {
    height: 185rpx;
}

/* 单个商品数量 */

.list_num {
    height: 40rpx;
    font-size: 25rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(255, 255, 255, 0.6);
    position: absolute;
    right: 7rpx;
    top: 145rpx;
}

    height: 90rpx;
    line-height: 90rpx;
}

/* 总和居中 */

.count_cent {
    position: relative;
    width: 90%;
    display: inline-block;
    height: 90rpx;
    line-height: 90rpx;
    border-top: 2rpx solid #31333f;
}

/* 商品数量 */

.cont_cent_name {
    position: relative;
    font-size: 28rpx;
    float: right;
    color: rgba(255, 255, 255, 0.6);
}

.cont_ce_mon {
    font-size: 32rpx;
    color: #d33d3c;
}

/* 总计金额 */

.cont_cen_money {
    position: relative;
    font-size: 28rpx;
    margin-right: 15rpx;
    margin-left: 15rpx;
    float: right;
    color: rgba(255, 255, 255, 0.6);
}

/* 标签功能 */

.coent_list {
    width: 700rpx;
    /* height: 130rpx; */
    line-height: 67rpx;

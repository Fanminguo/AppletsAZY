/* 退款退货中 */

.refund {
    z-index: 1;
    display: block;
    font-size: 0.8rem;
    color: #d33d3c;
    display: inline-block;
	float: left;
}

/* 去支付按钮 */

.btnColor {
    color: #fff !important;
    background: #d33d3c !important;
    border: none !important;
}

/* 已提醒 */

.remind {
    background-color: #161824 !important;
    color: #fff !important;
    border: 0;
}

/* 列表图片 */

.list_img {
    position: absolute;
    width: 144rpx;
    height: 144rpx;
    left: 0rpx;
    top: 30rpx;
	border-radius:5rpx; 
}

/* 列表标题 */

.list_name {
    position: absolute;
    text-align: left;
    top: 30rpx;
    left: 180rpx;
    color: rgba(255, 255, 255, 0.9);
    width: 350rpx;
    font-size: 28rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

/* 规格 */

.list_ions {
    position: absolute;
    text-align: left;
    top: 70rpx;
    left: 180rpx;
    color: rgba(255, 255, 255, 0.9);
    width: 350rpx;
    font-size: 24rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

/* 列表价格 */

.list_money {
    position: absolute;
    font-size: 32rpx;
    left: 170rpx;
    bottom: 30rpx;
    color: #d33d3c;

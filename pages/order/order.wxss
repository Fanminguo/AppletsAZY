/* 顶部菜单切换 */

page {
    background: #161824;
}

.cont_Image {
    width: 750rpx;
    height: 1090rpx;
    margin-top: -20rpx;
}

.none_tab image {
    display: block;
    margin: 0 auto;
    margin-top: 190rpx;
    padding-buttom: 190rpx;
    width: 530rpx;
    height: 334rpx;
}

.navbar {
    display: flex;
    background: #161824;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99;
    width: 750rpx;
}

.none_tab {
    color: #999;
    font-size: 32rpx;
}

/* 默认选中菜单 */

.navbar .item {
    position: relative;
    flex: auto;
    font-size: 27rpx;
    width: 100rpx;
    text-align: center;
    line-height: 80rpx;
    color: rgba(255, 255, 255, 0.6);
    /* border-bottom: 6rpx solid #ddd; */
}

/* 选中菜单样式 */

.navbar .item.active {
    color: rgba(250, 250, 250, 0.9);
}

/* 字体 */

.navbar .item.active:after {
    content: "";
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4rpx;
}

.total {
    margin-top: 110rpx;
}

/* 全部 */

.cont_count {
    padding-top: 6rpx;
    position: relative;
    text-align: center;
    margin: 0 auto;
    margin-top: 25rpx;
    background-color: #1d1f2b;
}

/* 编号 */

.seria {
    position: relative;
    display: inline-block;
    width: 90%;
    height: 100rpx;
    line-height: 100rpx;
    /* background-color: yellow; */
}

/* 订单lab */

.seriac_name {
    position: absolute;
    font-size: 28rpx;
    color: rgba(255, 255, 255, 0.9);
    left: 0rpx;
}

.seriac_name image {
    width: 25rpx;
    height: 23rpx;
}

.seriac_name text {
    margin-left: 15rpx;
}

/* 订单状态 */

.seriac_start {
    position: absolute;
    right: 0rpx;
    color: #d33d3c;
    font-size: 28rpx;
}

.seriac_start image {
    width: 34rpx;
    height: 31rpx;
}

/* 列表 */

.list {
    position: relative;
    width: 90%;
    display: inline-block;
    padding-bottom: 10rpx;
    height: 200rpx;
}

/* 按钮颜色 */


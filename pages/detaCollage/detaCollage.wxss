/* pages/detaCollage/detaCollage.wxss */
page {
    background: #161824;
}

/* 头部 */

.topInform {
    width: 750rpx;
    height: 290rpx;
}

.topImg {
    position: relative;
    margin: 20rpx 20rpx;
}

.topImg .commodity {
    width: 260rpx;
    height: 260rpx;
    float: left;
    border-radius: 20rpx;
}

.topImg .state {
    position: absolute;
    right: 16rpx;
    top: 60rpx;
    width: 180rpx;
    height: 145rpx;
}

.topImg .groupNum {
    width: 90rpx;
    height: 40rpx;
    background-color: #d33d3c;
    border-radius: 0 8rpx 8rpx 0;
    text-align: center;
    line-height: 40rpx;
    color: #fff;
    font-size: 26rpx;
    position: absolute;
    top: 20rpx;
    left: -4rpx;
}

/* 规格 */

.topLeft {
    width: 300rpx;
    margin-left: 30rpx;
    float: left;
}

.topLeft .name {
    font-size: 30rpx;
    color: rgba(255, 255, 255, 0.9);
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
	width: 400rpx;
	height: 74rpx;
}

.topLeft .groupSpec {
    font-size: 26rpx;
    color: #666;
    margin-bottom: 10rpx;
}

.topLeft .group_Num {
    font-size: 26rpx;
    color: rgba(255, 255, 255, 0.6);
    margin-bottom: 15rpx;
}

.topLeft .groupFree {
    display: inline-block;
    width: 70rpx;
    height: 30rpx;
    background-color: #d33d3c;
    font-size: 24rpx;
    color: #fff;
    border-radius: 4rpx;
    line-height: 35rpx;
    text-align: center;
}

.topLeft .groupMober {
    margin-top: 76rpx;
    font-size: 40rpx;
    color: #fff;
    margin-left: -10rpx;
}

.topLeft .groupMober .delete {
    font-size: 20rpx;
    color: #fff;
    text-decoration: line-through;
	margin-left: 20rpx;
}

/* 邀请好友 */

.groupNumTimeTit {
    text-align: center;
    font-size: 28rpx;
    color: rgba(255, 255, 255, 0.6);
    padding: 28rpx 0 16rpx 0;
}

/* 倒计时 */

.countDown {
    text-align: center;
    margin-bottom: 20rpx;
    line-height: 26rpx;
    margin-top: 10rpx;
	color:rgba(255,255,255,0.6);
}

.countDown text {
    padding-left: 20rpx;
    padding-right: 20rpx;
}

.partiCipants {
    width: 100%;
    text-align: center;
    margin: 0 auto;
    margin-top: 55rpx;
}

.partiCipants view {
    margin: 0 auto;
}

.partiCipants .fullName {
    margin: 0 20rpx;
    height: 150rpx;
    position: relative;
    display: inline-block;
}

.partiCipants .fullName .name {
    display: block;
    width: 100%;
    text-align: center;
    font-size: 28rpx;
    height: 50rpx;
    line-height: 50rpx;
    position: absolute;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.partiCipants .fullName image {
    width: 104rpx;
    height: 104rpx;
    border-radius: 50%;
    margin-top: 45rpx;
}

.partiCipants .fullName  .founder {
    position: absolute;
    display: block;
    left: 18rpx;
    top: 134rpx;
    width: 69rpx;
    height: 24rpx;
    line-height: 24rpx;
    background: rgba(247, 181, 0, 1);
    border-radius: 12px;
    color: #fff;
    font-size: 20rpx;
}

/* 参团按钮 */

.groupLink {
    height: auto;
    padding: 40rpx 120rpx 40rpx 120rpx;
    position: relative;
    margin-bottom: 24rpx;
}

.groupLink .goButton {
    display: block;
    line-height: 74rpx;
    background: #d33d3c;
    border-radius: 50rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    margin-bottom: 30rpx;
}

.groupLink .goList {
    background: rgba(211, 61, 52, 1);
    color: #fff;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    display: block;
    line-height: 74rpx;
    border-radius: 50rpx;
    text-align: center;
    font-size: 30rpx;
    margin-bottom: 30rpx;
}

/* 拼团玩法 */

.boxRule {
    border-top: 1rpx solid rgba(0, 0, 0, 0.1);
    margin-bottom: 20rpx;
    background-color: #161824;
}

.boxRule .similarBlock {
    display: block;
    font-size: 26rpx;
    line-height: 87rpx;
    height: 87rpx;
    text-align: center;
    color: #fff;
    margin: 0 auto;
}

/* 规则 */

.rule-wraper {
    width: 100%;
    border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
    padding-bottom: 20rpx;
}

.rule_tips {
    padding-bottom: 20rpx;
    padding-top: 20rpx;
    width: 580rpx;
    margin: 0 auto;
    height: 120rpx;
}

.rule_tips_list {
    width: 105rpx;
    color: #747474;
    font-size: 20rpx;
    text-align: center;
    float: left;
    /* display: inline-block */
}

.rule_tips_list image {
    width: 62rpx;
    height: 62rpx;
    margin: 0 auto;
    display: block;
}

.ruleTwo {
    width: 52rpx;
    /* display: inline-block; */
    float: left;
}

.ruleTwo image {
    width: 52rpx;
    height: 16rpx;
    margin-top: 25rpx;
}

.discount {
    color: #fff;
    text-align: center;
    height: 70rpx;
    line-height: 70rpx;
    width: 100%;
    font-size: 20rpx;
}

.discount image {
    width: 20rpx;
    height: 20rpx;
    display: inline-block;
}

/* 弹出框 */

.maskLayer {
    display: block;
    position: fixed;
    z-index: 21;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;

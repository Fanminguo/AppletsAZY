/* pages/cardCoupon/cardCoupon.wxss */

/*tab切换*/

.topTeb {
    width: 750rpx;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    background:rgba(22,24,36,1);
    box-shadow: 0 1rpx 2rpx 0 rgba(0, 0, 0, 0.2);
    border-radius: 0 0 26rpx 26rpx;
    position: fixed;
    top: 0;
    z-index: 99;
}

.nullImg {
    width: 519rpx;
    height: 296rpx;
    margin: 0 auto;
    margin-top: 134rpx;
}

.nullImg image {
    width: 519rpx;
    height: 334rpx;
}
.nullImg .topText{
font-size:34rpx;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(255,255,255,0.9);
text-align: center;
margin: 0 auto;
margin-top: 108rpx;
}
.nullImg .text{
font-size:28rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(255,255,255,0.6);
text-align: center;
margin: 0 auto;
margin-top: 20rpx;
}
.box {
    position: relative;
    top: 120rpx;
}

.topTeb view {
    float: left;
    width: 49%;
    height: 100rpx;
    color: rgba(155, 155, 155, 1);
    font-size: 32rpx;
    font-family: PingFangSC;
    font-weight: 500;
}

.active {
    color: #fff !important;
}

.quanList {
    width: 700rpx;
    height: 198rpx;
    margin: 0 auto;
    background: url(https://bj.aizhiyi.com/wap/test/wap/xcxImg/daijinquan.png) no-repeat;
    background-size: 100% 100%;
    margin-top: 10rpx;
}

.quanList .quanLeft {
    width: 212rpx;
    float: left;
	height: 198rpx;
	line-height: 198rpx;
}

.quanList .money {
    font-size: 38rpx;
    color: rgba(255, 255, 255, 1);
    padding-left: 57rpx;
}

.quanList .money text {
    height: 62rpx;
    font-size: 60rpx;
    font-family: Arial-BoldMT;
    font-weight: normal;
    line-height: 62rpx;
}

.quanList .reduction {
    height: 45rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    color: rgba(255, 255, 255, 1);
    line-height: 40rpx;
    padding-top: 20rpx;
    padding-left: 39rpx;
}

.appPackage {
    float: left;
    width: 327rpx;
    margin: 0 auto;
    color: #fff;
    padding-left: 50rpx;
    padding-top: 24rpx;
}

.appPackage .appText {
    height: 50rpx;
    font-size: 36rpx;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    line-height: 50rpx;
}

.appPackage .range {
    height: 40rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
	margin-top: 11rpx;
}

.quanList .tiem {
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
    line-height: 93rpx;
    padding-left: 39rpx;
}

/* 礼品卡 */

.giftCard {
    width: 730rpx;
    height: 367rpx;
    margin: 0 auto;
    background:linear-gradient(313deg,rgba(248,201,65,1) 0%,rgba(252,134,93,1) 49%,rgba(255,94,113,1) 100%);
    border-radius: 26rpx;
    padding-top: 10rpx;
    color: #fff;
    margin-bottom: 20rpx;
}

.giftCard .cardInfo {
    width: 400rpx;
    height: 200rpx;
    margin: 0 auto;
    text-align: center;
    margin-top: 40rpx;
}

.cardInfo .available {
    height: 45rpx;
    font-size: 32rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
    line-height: 45rpx;
}

.cardInfo .available image {
    width: 38rpx;
    height: 26rpx;
}

/* 可用金额 */

.cardInfo .money {
    height: 70rpx;
    font-size: 50rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    line-height: 70rpx;
    margin-top: 11rpx;
}

.cardInfo .money .moneyText {
    font-size: 32rpx;
}

/* 冻结金额 */

.frozen {
    font-size: 28rpx;
    background: rgba(0, 0, 0, 0.08);
    width: 200rpx;
    border-radius: 20rpx;
    margin: 0 auto;
    padding: 5rpx 20rpx;
	margin-top: 10rpx;
}

.cumLeft {
    width: 49%;
    float: left;
	text-align: center;
    margin-top: 10rpx;
}

.cumLeft .text {
    font-size: 28rpx;
    color:rgba(255,255,255,0.8);
    margin-bottom: 5rpx;
}

.cumRight {
    width: 49%;
    float: left;
	text-align: center;
    margin-top: 10rpx;
}

.cumRight .text {
    font-size: 28rpx;
    margin-bottom: 5rpx;
    color:rgba(255,255,255,0.8);
}

/* 列表 */

.recordList {
    width: 700rpx;
    margin: 0 auto;
    margin-top: 120rpx;
    position: absolute;
    top: 350rpx;
    left: 20rpx;
    z-index: -5;
}

.recor {
    height: 110rpx;
    margin-top: 10rpx;
    border-bottom: 1rpx solid #31333F;
}

.listTop {
    margin-top: 30rpx;
}

.listTop .left {
    float: left;
    height: 40rpx;
    font-size: 28rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color:rgba(255,255,255,0.9);
    line-height: 40rpx;
}

.listTop  .right {
    float: right;
    height: 42rpx;
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 600;
   color:rgba(179,178,178,1);
    line-height: 42rpx;
}

.listRight {
    margin-top: 10rpx;
}

.listRight .left {
    float: left;
    height: 33rpx;
    font-size: 24rpx;
    font-family: PingFangSC;
    font-weight: 500;
    color: rgba(153, 153, 153, 1);
    line-height: 33rpx;
}

.listRight .right {
    float: right;
    font-size: 30rpx;
    font-family: PingFangSC;
    font-weight: 600;
    color: rgb(148, 147, 147);
}

/* 增加 */

.increase {
    height: 42rpx !important;
    font-size: 30rpx !important;
    font-family: PingFangSC;
    font-weight: 600 !important;
    color: rgba(43, 186, 63, 1) !important;
    line-height: 42rpx !important;
}

/*  底部*/

.btnButtom {
    width: 750rpx;
    height: 115rpx;
    background:rgba(22,24,36,1);
    box-shadow: 0 1rpx 6rpx 0 rgba(0, 0, 0, 0.2);
    line-height: 115rpx;
    position: fixed;
    bottom: 0;
    text-align: center;
	color: #fff;
}

/*使屏幕变暗 */

.commodity_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background:rgba(0,0,0,0.6);
    overflow: hidden;
    z-index: 1000;
    color: #fff;
}

/*对话框 */

.commodity_attr_box {
    height: 460rpx;
    width: 100%;
    overflow: hidden;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
    background:rgba(22,24,36,1);
    border-radius: 12px 12px 0px 0px;
    padding-top: 20rpx;
}

.denseText {
    width: 170rpx;
    height: 48rpx;
    font-size: 34rpx;
    font-family: PingFangSC;
    font-weight: 600;
    color: #fff;
    line-height: 48rpx;
    text-align: center;
    margin: 0 auto;
}

.denseInput {
    width: 600rpx;
    height: 76rpx;
    margin: 0 auto;
    margin-top: 108rpx;
}

.denseInput input {
    border-radius: 12rpx;
    border: 1rpx solid rgba(62, 53, 53, 1);
    text-align: center;
    height: 76rpx;
    line-height: 76rpx;
	color: #fff;
}

.denseBtn {
    width: 750rpx;
    height: 115rpx;
    background: #161824;
    box-shadow: 0 1rpx 6rpx 0 rgba(0, 0, 0, 0.2);
	border-top: 1rpx solid rgba(0,0,0,0.2);
    font-size: 32rpx;
    font-weight: 500;
    text-align: center;
    margin-top: 108rpx;
}

.denseBtn text {
    line-height: 115rpx;
	color:rgba(202,73,58,1) !important;
}

/* 错误提示信息 */

.error {
    font-size: 26rpx;
    color: #d33d3c;
    margin: 0 auto;
    text-align: center;
    padding-top: 25rpx;
}
.listkong{width: 100%;margin-top: 200rpx;}
.textA{color: #fff; font-size: 36rpx; text-align: center;}
.textB{color: #fff; font-size: 28rpx; text-align: center; margin-top: 15rpx;}
.bgImg{
    width: 30rpx;
    height: 68rpx;
    background: url(https://bj.aizhiyi.com/wap/test/wap/xcxImg/jiantou@2x.png) no-repeat;
    background-size: 100% 100%;
    margin: 0 auto;
    margin-top: 83rpx;
}

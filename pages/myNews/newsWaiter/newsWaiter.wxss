page {
    background: rgba(22, 24, 36, 1);
}

.empty image {
    display: block;
    margin: 0 auto;
    margin-top: 190rpx;
    width: 530rpx;
    height: 334rpx;
}

.empty .view {
    margin: 0 auto;
    margin-top: 108rpx;
    font-size: 34rpx;
    color: rgba(255, 255, 255, 0.9);
    text-align: center;
    height: 48rpx;
    line-height: 48rpx;
}

.empty .text {
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(255, 255, 255, 0.6);
    line-height: 40rpx;
    margin-top: 20rpx;
    text-align: center;
}

.newBox {
    width: 750rpx;
}

.newBox .tiem {
    width: 410rpx;
    height: 59rpx;
    background: #1D1F28;
    border-radius: 26rpx;
    margin: 30rpx auto 20rpx auto;
    line-height: 59rpx;
    text-align: center;
    color: rgba(255, 255, 255, 0.6);
}

.newBox .boxText {
    width: 702rpx;
    height: 233rpx;
    background: #1D1F28;
    border-radius: 26rpx;
    margin: 0 auto;
}

.newBox .boxText .state {
    height: 40rpx;
    font-size: 28rpx;
    font-weight: 500;
    color: rgba(255, 255, 255, 0.9);
    line-height: 40rpx;
    margin-top: 20rpx;
    margin-left: 20rpx;
    margin-bottom: 35rpx;
}

.newBox .boxText .boxImg {
    float: left;
}

.newBox .boxText .boxImg image {
    width: 124rpx;
    height: 124rpx;
    border-radius: 16rpx;
    margin-left: 20rpx;
}

.boxName {
    width: 467rpx;
    margin-left: 25rpx;
    float: left;
    color: rgba(255, 255, 255, 0.9);
}

.boxName .name {
    height: 80rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(98, 98, 98, 1);
    line-height: 40rpx;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
}

.boxName .btn {
    height: 40rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #9b9b9b;
    line-height: 40rpx;
    margin-top: 10rpx;
}

.remind {
    height: 100rpx;
    font-size: 24rpx;
    font-family: PingFangSC;
    font-weight: 400;
    color: rgba(153, 153, 153, 1);
    line-height: 100rpx;
    width: 750rpx;
    text-align: center;
}